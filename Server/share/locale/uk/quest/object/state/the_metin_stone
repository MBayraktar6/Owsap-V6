the_metin_stone={["start"]=0,["__COMPLETE__"]=668443392,["goto_myonghorang"]=1826792210,["run"]=1349952704,update_counter= function ()local count = 0 
local remaining_count = 1 - pc . getqf ( "hunted_metin_stone" ) 
if remaining_count < 0 then 
remaining_count = 0 
end 
count = count + remaining_count 
q . set_counter_value ( count ) 
if count == 0 then 
notice_multiline ( c_locale ( 6394 ) , notice ) 
set_state ( "goto_myonghorang" ) 
end 
end }